{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true
    },
        "extends": [
        "airbnb-base", "prettier" 
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "import/prefer-default-export" : "off",
        "prettier/prettier": [
            "error",
            {
              "endOfLine": "auto",
              "export": "ana"            
            }
            
          ],
        "no-console":"off",
        "object-shorthand":"off",
        "func-names":"off",
        
        "no-unused-vars": "warn",
        "max-classes-per-file": "off",
        "no-new": "off",
        "no-dynamic-require": "off",
        "import/no-dynamic-require": "off",
        "no-param-reassign": "off"
        }
    }
        
    
