{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true
    },
        "extends": [
        "airbnb-base", "prettier" 
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "import/prefer-default-export" : "off",
        "prettier/prettier": [
            "error",
            {
              "endOfLine": "auto",
              "export": "ana"            
            }
            
          ],
        "no-console":"off",
        "no-unused-vars": "warn"
    }
}