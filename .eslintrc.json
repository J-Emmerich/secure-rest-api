{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true
    },
        "extends": [
        "airbnb-base", "prettier" 
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
              "endOfLine": "auto"
            }
          ],
        "no-console":"off",
        "no-unused-vars": "warn"
    }
}